{
  "name": "@speechkit/speechkit-audio-player-v2",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpeechKit/speechkit-audio-player-v2.git",
    "directory": "packages/player"
  },
  "scripts": {
    "build": "yarn run build:module & yarn run build:lib & yarn run build:ie11",
    "build:module": "NODE_ENV=production rollup -c",
    "build:ie11": "NODE_ENV=production rollup -c rollup-ie11.config.js",
    "build:lib": "NODE_ENV=production RELEASE=1 rollup -c rollup-libs.config.js --environment INCLUDE_DEPS:false,BUILD:production",
    "develop": "rollup -c -w",
    "develop:lib": "rollup -c rollup-libs.config.js -w",
    "develop:ie11": "rollup -c rollup-ie11.config.js -w",
    "start": "sirv ../../demo"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@koa/cors": "^3.1.0",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^1.3.3",
    "core-js": "3",
    "redux-logger": "^3.0.6",
    "rollup": "^2.6.1",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-ignore": "^1.0.5",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-strip-code": "^0.2.7",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "memoize-one": "^5.1.1",
    "mitt": "^1.2.0",
    "redux": "^4.0.5",
    "redux-act": "^1.8.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "svelte": "^3.31.0",
    "svelte-redux-connect": "^1.2.0"
  },
  "files": [
    "build"
  ],
  "modules": "build/cjs",
  "esmodules": "build/module",
  "system": "build/nomodule"
}
